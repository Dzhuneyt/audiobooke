{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --progress=false",
    "build:dev:browser:watch": "ng run frontend:build --watch --progress",
    "build:dev:server:watch": "ng run frontend:server:development --watch",
    "build:prod:browser:watch": "./node_modules/@angular/cli/bin/ng run frontend:build:production --watch --delete-output-path",
    "build:prod:server:watch": "./node_modules/@angular/cli/bin/ng run frontend:server:production --watch --output-hashing=none --delete-output-path",
    "build:prod:server:post-build": "nodemon --verbose --watch './dist/**/main.js' --exec 'npm run build:prod:server:webpack && npm run serve:ssr'",
    "build:prod:server:webpack": "wait-on --window 2000 ./dist/server/main.js && webpack --config webpack.server.config.js --progress --colors",
    "wait-for-server": "wait-on --window 1200 ./dist/server/main.js && webpack --config webpack.server.config.js --progress --colors",
    "dev:ssr:DEPRECATED": "npm-run-all -p -l build:dev:browser:watch build:dev:server:watch build:prod:server:post-build",
    "dev:ssr": "npm run tmp:cleanup && echo 'something' && ngcc && ng run frontend:serve-ssr",
    "dev:lite-server": "lite-server -c ./bs-config.json",
    "test": "./node_modules/@angular/cli/bin/ng test --sourceMap=false",
    "test:cli": "./node_modules/@angular/cli/bin/ng test --sourceMap=false --watch=false --progress=false --browsers=ChromeHeadless",
    "lint": "./node_modules/@angular/cli/bin/ng lint",
    "e2e": "ng e2e",
    "build-and-watch:ssr": "npm run build:ssr && npm run serve:ssr",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "./node_modules/@angular/cli/bin/ng build --prod && ng run frontend:server:production",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run frontend:server:production",
    "prerender": "ng run frontend:prerender",
    "tmp:cleanup": "rm -rf node_modules/@angular/compiler-cli/ngcc/__ngcc_lock_file__ || true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/platform-server": "9.1.0",
    "@angular/router": "9.1.0",
    "@nguniversal/common": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
    "@yellowspot/ng-truncate": "^1.5.0",
    "angular-2-local-storage": "^3.0.2",
    "apollo-angular": "^1.9.1",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "core-js": "^3.6.4",
    "express": "^4.15.2",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.0",
    "lite-server": "^2.5.4",
    "pretty-ms": "^6.0.1",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@nguniversal/builders": "^9.1.0",
    "@types/express": "^4.17.0",
    "@types/google.analytics": "0.0.40",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "chokidar-cli": "^2.1.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "nodemon": "^2.0.2",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "protractor": "~5.4.3",
    "ts-loader": "^6.2.2",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.1",
    "typescript": "3.8.3",
    "wait-on": "^4.0.1",
    "webpack-cli": "^3.1.0"
  }
}
